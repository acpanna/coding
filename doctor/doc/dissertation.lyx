\begin_layout Theorem
\begin_inset CommandInset label
LatexCommand label
name "thm:ProjektionsRekursionsformel"

\end_inset

Es seien 
\begin_inset Formula $b_{1},b_{2},...,b_{m}\in\mathbb{R}^{n}$
\end_inset

 die Basisvektoren des Gitters und 
\begin_inset Formula $b\in span(b_{1},b_{2},...,b_{m})$
\end_inset

, d.h.
 
\begin_inset Formula $b=\sum_{i=1}^{m}x_{i}b_{i}$
\end_inset

 mit 
\begin_inset Formula $x\in\mathbb{R}^{m}$
\end_inset

.
 
\end_layout

\begin_layout Theorem
Dann ist:
\begin_inset Formula \[
\pi_{k}(b)=\pi_{k}\Biggl(\sum_{i=k}^{m}x_{i}b_{i}\Biggr)=\Biggl(\sum_{i=k}^{m}x_{i}\mu_{i,k}\Biggr)\cdot\hat{b}_{k}+\pi_{k+1}\Biggl(\sum_{i=k+1}^{m}x_{i}b_{i}\Biggr)\]

\end_inset


\end_layout

\begin_layout Proof
Nach 
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:orthogonale_Projektion"

\end_inset

 gilt einerseits: 
\begin_inset Formula $\pi_{k}(b)=\sum_{j=k}^{m}\frac{\langle b,\hat{b_{j}}\rangle}{\langle\hat{b}_{j},\hat{b}_{j}\rangle}\hat{b}_{j}$
\end_inset

 
\end_layout

\begin_layout Proof
Andererseits ist aber:
\begin_inset Formula \begin{align*}
\pi_{k}\Biggl(\sum_{i=k}^{m}x_{i}b_{i}\Biggr) & =\sum_{i=k}^{m}x_{i}\cdot\pi_{k}(b_{i})=\sum_{i=k}^{m}x_{i}\cdot\Biggl(\sum_{j=k}^{i}\mu_{i,j}\hat{b}_{j}\Biggr)\end{align*}

\end_inset

Umordnung der Summationsreihenfolge ergibt:
\begin_inset Formula \begin{align*}
\sum_{i=k}^{m}x_{i}\cdot\Biggl(\sum_{j=k}^{i}\mu_{i,j}\hat{b}_{j}\Biggr) & =\sum_{j=k}^{m}\hat{b}_{j}\cdot\Biggl(\sum_{s=j}^{m}x_{s}\cdot\mu_{s,j}\Biggr)=\sum_{j=k}^{m}\hat{b}_{j}\cdot\frac{\langle b,\hat{b}_{j}\rangle}{\langle\hat{b}_{j},\hat{b}_{j}\rangle}\end{align*}

\end_inset

Also ist:
\begin_inset Formula \begin{align*}
\pi_{k}(b) & =\pi_{k}(\sum_{i=k}^{m}x_{i}b_{i})\end{align*}

\end_inset


\end_layout

\begin_layout Proof
Der zweite Teil der Gleichung ergibt sich folgendermaßen: 
\begin_inset Formula \begin{align*}
\pi_{k}\Biggl(\sum_{i=k}^{m}x_{i}b_{i}\Biggr) & =\sum_{i=k}^{m}x_{i}\cdot\Biggl(\sum_{j=k}^{i}\mu_{i,j}\hat{b}_{j}\Biggr)=\sum_{i=k}^{m}x_{i}\cdot\Biggl(\mu_{i,k}\hat{b}_{k}+\sum_{j=k+1}^{i}\mu_{i,j}\hat{b}_{j}\Biggr)\\
 & =\Biggl(\sum_{i=k}^{m}x_{i}\mu_{i,k}\Biggr)\cdot\hat{b}_{k}+\sum_{i=k}^{m}x_{i}\Biggr(\sum_{j=k+1}^{i}\mu_{i,j}\hat{b}_{j}\Biggr)\end{align*}

\end_inset


\end_layout

\begin_layout Proof
Da die letzte Summe für 
\begin_inset Formula $i=k$
\end_inset

 verschwindet, ergibt sich unter Anwendung von 
\begin_inset Formula $(*)$
\end_inset

 letztendlich:
\begin_inset Formula \begin{align*}
\pi_{k}\Biggl(\sum_{i=k}^{m}x_{i}b_{i}\Biggr) & =\Biggl(\sum_{i=k}^{m}x_{i}\mu_{i,k}\Biggr)\cdot\hat{b}_{k}+\sum_{i=k+1}^{m}x_{i}\Biggr(\sum_{j=k+1}^{i}\mu_{i,j}\hat{b}_{j}\Biggr)\\
 & =\Biggl(\sum_{i=k}^{m}x_{i}\mu_{i,k}\Biggr)\cdot\hat{b}_{k}+\pi_{k+1}\Biggl(\sum_{i=k+1}^{m}x_{i}b_{i}\Biggr)\end{align*}

\end_inset


\end_layout

\begin_layout Lemma
\begin_inset CommandInset label
LatexCommand label
name "lem:exausOrthProj"

\end_inset

 Für einen beliebigen Vektor 
\begin_inset Formula $b=\sum_{i=1}^{m}x_{i}b_{i}$
\end_inset

 gilt:
\begin_inset Formula \[
\langle\pi_{j}(b),\pi_{k}(b)\rangle=||\pi_{j}(b)||_{2}^{2}\mbox{ für alle }1\leq k<j\leq m\]

\end_inset


\end_layout

\begin_layout Proof
\begin_inset Formula \begin{align*}
\langle\pi_{j}(b),\pi_{k}(b)\rangle & =\left\langle \sum_{h=j}^{m}\frac{\langle b,\hat{b}_{h}\rangle}{\langle\hat{b}_{h},\hat{b}_{h}\rangle}\hat{b}_{h},\sum_{l=k}^{m}\frac{\langle b,\hat{b}_{l}\rangle}{\langle\hat{b}_{l},\hat{b}_{l}\rangle}\hat{b}_{l}\right\rangle \\
 & =\sum_{h=j}^{m}\sum_{l=k}^{m}\left\langle \frac{\langle b,\hat{b}_{h}\rangle}{\langle\hat{b}_{h},\hat{b}_{h}\rangle}\hat{b}_{h},\frac{\langle b,\hat{b}_{l}\rangle}{\langle\hat{b}_{l},\hat{b}_{l}\rangle}\hat{b}_{l}\right\rangle \\
 & =\sum_{h=j}^{m}\sum_{l=k}^{m}\frac{\langle b,\hat{b}_{h}\rangle}{\langle\hat{b}_{h},\hat{b}_{h}\rangle}\cdot\frac{\langle b,\hat{b}_{l}\rangle}{\langle\hat{b}_{l},\hat{b}_{l}\rangle}\cdot\langle\hat{b}_{h},\hat{b}_{l}\rangle\end{align*}

\end_inset


\end_layout

\begin_layout Proof
Für alle 
\begin_inset Formula $l=k,k+1,...,j-1$
\end_inset

 ist 
\begin_inset Formula $\langle\hat{b}_{j},\hat{b}_{l}\rangle=0$
\end_inset

 und somit:
\begin_inset Formula \begin{align*}
\langle\pi_{j}(b),\pi_{k}(b)\rangle= & \sum_{h=j}^{m}\sum_{l=j}^{m}\frac{\langle b,\hat{b}_{h}\rangle}{\langle\hat{b}_{h},\hat{b}_{h}\rangle}\cdot\frac{\langle b,\hat{b}_{l}\rangle}{\langle\hat{b}_{l},\hat{b}_{l}\rangle}\cdot\langle\hat{b}_{h},\hat{b}_{l}\rangle\\
= & \left\langle \sum_{h=j}^{m}\frac{\langle b,\hat{b}_{h}\rangle}{\langle\hat{b}_{h},\hat{b}_{h}\rangle}\hat{b}_{h},\sum_{l=j}^{m}\frac{\langle b,\hat{b}_{l}\rangle}{\langle\hat{b}_{l},\hat{b}_{l}\rangle}\hat{b}_{l}\right\rangle \\
= & \langle\pi_{j}(b),\pi_{j}(b)\rangle=||\pi_{j}(b)||_{2}^{2}\end{align*}

\end_inset


\end_layout

\begin_layout Theorem
\begin_inset CommandInset label
LatexCommand label
name "thm:exausErw"

\end_inset

 Es seien 
\begin_inset Formula $u_{t},u{}_{t+1},...,u_{m}\in\mathbb{Z}$
\end_inset

 festgewählt.
 Wenn 
\begin_inset Formula $u_{1},u_{2},...,u_{t-1}$
\end_inset

 
\begin_inset Formula $\in\mathbb{Z}$
\end_inset

 existieren, so dass für 
\begin_inset Formula $b=\sum_{i=1}^{m}u_{i}b_{i}$
\end_inset

 die Ungleichung 
\begin_inset Formula $||b||_{p}\leq F_{p}$
\end_inset

 erfüllt ist, dann gilt für alle 
\begin_inset Formula $y_{t},y_{t+1},...,y_{m}\in\mathbb{R}$
\end_inset

: 
\begin_inset Formula \[
\left|\sum_{i=t}^{m}y_{i}\cdot||\pi_{i}(b)||_{2}^{2}\right|\leq F_{p}\cdot||\sum_{i=t}^{m}y_{i}\cdot\pi_{i}(b)||_{q}\]

\end_inset


\end_layout

\begin_layout Theorem
wobei 
\begin_inset Formula $F_{p}>0$
\end_inset

, 
\begin_inset Formula $1\leq p,q\leq\infty$
\end_inset

 und 
\begin_inset Formula $\frac{1}{p}+\frac{1}{q}=1$
\end_inset

.
\end_layout

\begin_layout Proof
Nach Lemma 
\begin_inset CommandInset ref
LatexCommand ref
reference "lem:exausOrthProj"

\end_inset

 gilt für alle 
\begin_inset Formula $k<i$
\end_inset

:
\begin_inset Formula \[
\langle\pi_{k}(b),\pi_{i}(b)\rangle=||\pi_{i}(b)||_{2}^{2}\]

\end_inset

Wenn 
\begin_inset Formula $||b||_{p}=||\pi_{1}(b))||_{p}\leq F_{p}$
\end_inset

 dann folgt mit der Hölder'schen Ungleichung
\begin_inset ERT
status collapsed

\begin_layout Plain Layout


\backslash
index{Hölder'sche Ungleichung}
\end_layout

\end_inset

:
\begin_inset Formula \begin{eqnarray*}
\left|\sum_{i=t}^{m}y_{i}||\pi_{i}(b)||_{2}^{2}\right| & = & \left|\sum_{i=t}^{m}y_{i}\langle\pi_{1}(b),\pi_{i}(b)\rangle\right|=\left|\langle\pi_{1}(b),\sum_{i=t}^{m}y_{i}\pi_{i}(b)\rangle\right|\\
 & \leq & ||\pi_{1}(b)||_{p}\cdot||\sum_{i=t}^{m}y_{i}\pi_{i}(b)||_{q}\leq F_{p}\cdot||\sum_{i=t}^{m}y_{i}\pi_{i}(b)||_{q}\end{eqnarray*}

\end_inset


