\begin_inset Formula \[
1\cdot b_{1}+x_{1}b_{2}+x_{2}b_{3}+...+x_{k}b_{k+1}\in L\]

\end_inset


\end_layout

\begin_layout Proof
Also:
\begin_inset Formula \[
1\cdot b_{1}+x_{1}b_{2}+x_{2}b_{3}+...+x_{k}b_{k+1}\]

\end_inset


\begin_inset Formula \[
=(-d+A\cdot x,2c_{1}\cdot x_{1}-R,2c_{2}\cdot x_{2}-R,...,2\cdot c_{k}x_{k}-R,R)\]

\end_inset


