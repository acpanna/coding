\begin_inset Formula \[
\mathcal{O}\bigl(nm(m^{m/2+o(m)}+m^{2})\cdot\mbox{log }M\bigr),\]

\end_inset 


\begin_inset Formula \[
\mbox{wobei }M:=\underset{^{i=1,2,...,m}}{\mbox{max}}||b_{i}||^{2}.\]

\end_inset 


