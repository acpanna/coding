LINK	=	g++
LFLAGS	=	
LIBS	=	$(SUBLIBS) -L$(QTDIR)/lib -L/usr/X11R6/lib -L$(KDEDIR)/lib -lkdeui -lkdecore -lkfile -lkio -lqt -lXext -lX11 -lm
